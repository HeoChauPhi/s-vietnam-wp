<!DOCTYPE html>
<html>
  <head>
    <meta name="google-site-verification" content="PYOcd9lnlTaqjmSQxBG-oBCDvyQLGsjWSdDsPelxNwk" />
    <meta charset="{{site.charset}}" />
    {% if function('is_front_page') %}
      <title>{{site.name}} - {{site.description}}</title>
    {% else %}
      <title>{{ function('bloginfo') }} - {{ wp_title }}</title>
    {% endif %}
    <meta name="description" content="{{site.description}}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1, minimum-scale=1">
    <link rel="shortcut icon" href="{{site_favicon}}" />
    <link rel="pingback" href="{{site.pingback_url}}" />
    <meta property="fb:app_id" content="2122068604687989" />
    {{ wp_head }}
  </head>
  <body class="{{ body_class }}" data-template="base.twig">
    <script async defer src="http://maps.googleapis.com/maps/api/js?key={{ google_api_key }}&#38;libraries=places" type="text/javascript"></script>
    <div class="page-wrapper">
      {% block header %}
        {% include 'header.twig' %}
      {% endblock %}

      {% block feature %}
      {% endblock %}

      <main role="main" class="clearfix">
        {% block content %}
          {{context}}
        {% endblock %}
      </main>

      {% block footer %}
        {% include 'footer.twig' %}
        {{ wp_footer }}
      {% endblock %}

      {% block footer_scripts %}
      {% endblock %}
    </div>
    <div id="fb-root"></div>
